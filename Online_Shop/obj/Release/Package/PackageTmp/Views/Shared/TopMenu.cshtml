@model List<Model.EF.Menu>
@using Online_Shop.Common;

<div class="call">
    <p><span>Need help?</span> call us <span class="number">1-22-3456789</span></p>
</div>
<div class="account_desc">
    @{
        var session = (UserLogin)Session[CommonConstants.USER_SESSION_CLIENT];
    }
    @if (session == null)
    {
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
            }
        </ul>
    }
    else
    {
        <ul>
            <li>Xin chào: @session.UserName</li>
            <li>@Html.ActionLink("Đăng xuất", "Logout", "User")</li>
        </ul>
    }
</div>
<div class="clear"></div>

